# ForkedOcean Environment Variables
# Copy this file to .env.local and fill in your values

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://forkedocean.com
NEXT_PUBLIC_SITE_NAME=ForkedOcean
NEXT_PUBLIC_SITE_DESCRIPTION=Your Ocean. Your Control.

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=

# Feature Flags
NEXT_PUBLIC_ENABLE_TERMINAL=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Security
NEXT_PUBLIC_CSP_NONCE=